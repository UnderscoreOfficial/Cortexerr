{
  "name": "@cortexerr/core",
  "version": "0.0.1",
  "description": "Official API typescript definitions and js runtime files for Cortexerr Core Api.",
  "keywords": [
    "cortexerr",
    "types",
    "runtime",
    "typescript"
  ],
  "author": "UnderscoreOfficial",
  "repository": "github:UnderscoreOfficial/Cortexerr",
  "license": "GPL-3.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./merged/core/src/index.d.ts",
  "files": [
    "merged/core/src/**/*.d.ts",
    "dist/**/*.js"
  ],
  "exports": {
    ".": {
      "types": "./merged/core/src/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./args.js": {
      "types": "./merged/core/src/*.d.ts",
      "require": "./dist/args.js",
      "import": "./dist/args.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/test.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ctrl/qbittorrent": "^9.11.0",
    "@xmldom/xmldom": "^0.8.11",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^5.3.3",
    "pino": "^10.2.1",
    "pino-pretty": "^13.1.3",
    "undici": "^7.18.2",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@ctrl/shared-torrent": "^6.5.0"
  }
}
